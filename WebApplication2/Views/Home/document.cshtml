@* @model WebApplication2.Data.Requestwisefile *@@model IEnumerable<WebApplication2.Data.Requestwisefile>@using Microsoft.AspNetCore.Http@inject IHttpContextAccessor Accessor@{    ViewData["Title"] = "Document Page";}<nav class="bg-white">    <div class="container-fluid shadow">        <ul>            <li>                <img src="~/OneDrive_2024-01-17/SRS Screen Shorts/backremovelogo.png" alt="" height="100px" width="180">            </li>            <li class="hideonmobile me-2">Welcome <b> @Accessor.HttpContext.Session.GetString("Usarname")</b></li>            <li>                <a asp-controller="Home" asp-action="patientlogin">                    <button class="btn hideonmobile me-2"                            style="border:2px solid #54d4d3; color: #54d4d3;">                        Logout                    </button>                </a>            </li>            <li>                <a asp-controller="Home" asp-action="patientlogin">                    <button class="btn showonmobile hide me-2"                            style="border:2px solid #54d4d3;">                        <i class="fa-solid fa-arrow-right-from-bracket"                           style="color: #54d4d3;"></i>                    </button>                </a>            </li>            <li><button class="moonbtn backtra me-2"><i class="fa-solid fa-moon  "></i></button></li>        </ul>        <hr>        <ul class="nav nav-tabs" id="myTab" role="tablist">            <li class="nav-item" role="presentation">                <button class="nav-link active border-0 text-muted" id="home-tab" data-bs-toggle="tab"                        data-bs-target="#home" type="button" role="tab" aria-controls="home"                        aria-selected="true">                    <b>Dashboard</b>                </button>            </li>            <li class="nav-item" role="presentation">                <button class="nav-link text-muted border-0 " id="profile-tab" data-bs-toggle="tab"                        data-bs-target="#profile" type="button" role="tab" aria-controls="profile"                        aria-selected="false">                    <b>Profile</b>                </button>            </li>        </ul>    </div></nav><div class="tab-content">        <div class="tab-pane container active" id="dashboard">            <div class="container mx-0">                <div class="row">                    <div class="col-12">                        <div class="d-flex justify-content-between mt-3">                            <h4 class="mt-3">Documents</h4>                            <a class="text-start text-info text-decoration-none " asp-controller="Home" asp-action="patientdashboard">
                                <button class="btn btn-white  text-info border-info rounded ">
                                    <i class="fa-solid  fa-less-than"></i> Back
                                </button>
                            </a>                        </div>                        <div class="container  rounded bg-white shadow py-3">                            <h6 class="text-muted">Patient Name</h6>                            <h4 style="color:#54d4d3;">@Accessor.HttpContext.Session.GetString("Usarname")</h4>                            <h6 class="text-muted">Check here for any files that you or the doctors of your subsequents requestors have attached for you to review.</h6>                            <div class="row">                                <div class="row">                                    <div class="col">                                        <form method="post" asp-action="UploadFile"  asp-controller="Aspnetusers" enctype="multipart/form-data">                                            <div class="form-group row">                                                <fieldset id="upload">
                                                    <div class="input-group mb-3">
                                                        <input type="file" name="fileToUpload" class="form-control" id="fileToUpload">
                                                        <label class="input-group-text text-muted p-0" for="inputGroupFile02">                                                        <button class="btn btn-info mx-0">                                                            <i class="fa-solid fa-cloud-arrow-up" style="color: #ffffff;"></i>
                                                                Upload
                                                            </button>
                                                            @* <i class="btn btn-info fa-solid fa-cloud-arrow-up" style="color: #ffffff;"> Upload</i> *@
                                                        </label>
                                                    </div>
                                                </fieldset>                                            </div>                                        </form>                                    </div>                                </div>                                <div class="d-flex justify-content-between">                                    <h4 class="mt-3">Documents</h4>                                    <a asp-action="PatientDashboard" asp-controller="Home"><button class="btn btn-white text-info border-info rounded mt-3 mb-3 " style="height: 40px; width : 150px; color: #54B4D3;">Download All</button></a>                                </div>                                <table class="table hideOnMobile ">                                    <thead class="bg-light justify-content-center ">                                        <tr>                                            <th>                                                <input type="checkbox" class="ms-3" />                                            </th>                                            <th>                                                @Html.DisplayNameFor(model => model.Filename)                                            </th>                                            <th>                                                @Html.DisplayNameFor(model => model.Createddate)                                            </th>                                            <th class="text-center"> Action</th>                                        </tr>                                    </thead>                                    <tbody>                                        @foreach (var item in Model)
                                        {                                            <tr>                                                <td><input type="checkbox" class="ms-3" /></td>                                                <td>                                                    <i class="fa-solid fa-file-pdf" style="color: #ff0000;"></i>                                                    @Html.DisplayFor(modelItem => item.Filename)                                                </td>                                                <td>                                                    @Html.DisplayFor(modelItem => item.Createddate)                                                </td>                                                <td class="text-center">                                                    @*@Html.ActionLink("Click here to download", "DownloadFile", new { })*@                                                    <a asp-action="DownloadFile" asp-controller="Home"><button class="btn btn-white border-1 border-info px-1 "><i class="fa-solid fa-cloud-arrow-down" style="color: #54B4D3;"></i></button></a>                                                </td>                                            </tr>                                        }                                    </tbody>                                </table>                            </div>                        </div>                    </div>                </div>            </div>            <div class="tab-pane container fade" id="profile">this is Profile tab</div>        </div>        <script>            function changeTheme() {                if ($('#light-theme').css('display') == 'none') {                    document.querySelector(".login-bg").style.display = "block";                    document.querySelector(".login-bg-dark").style.display = "none";                }                else {                    document.querySelector(".login-bg").style.display = "none";                    document.querySelector(".login-bg-dark").style.display = "block";                }            }            function showSidebar() {                document.querySelector(".sideBar").style.display = "flex";            }            function hideSidebar() {                document.querySelector(".sideBar").style.display = "none";            }            function showHidePassword() {                let temp = document.getElementById("inputPassword");                if (temp.type === "password") {                    temp.type = "text";                }                else {                    temp.type = "password";                }            }        </script>    <script src="~/lib/jquery/dist/jquery.min.js"></script>    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>    <script src="~/js/site.js" asp-append-version="true"></script>